{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["node"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "outDir": "dist",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": ["src/**/*", "webpack.*.ts"],
  "exclude": ["node_modules", "dist", "out", "**/python-server/venv/Lib/site-packages/xlwings/js", "**/venv/**"]
}
