    "make:mac": "npm run build:python && electron-forge make --platform=darwin",
    "make:linux": "npm run build:python && electron-forge make --platform=linux",
    "publish": "npm run build:python && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "clean": "rimraf out"
  },
